{
	"name": "faker-cli",
	"private": true,
	"type": "module",
	"packageManager": "yarn@4.4.1",
	"devDependencies": {
		"@lusc/tsconfig": "^5.1.0",
		"@types/node": "^22.5.4",
		"esbuild": "^0.23.1",
		"prettier": "^3.3.3",
		"rimraf": "^6.0.1",
		"type-fest": "^4.26.1",
		"typescript": "^5.6.2",
		"xo": "^0.59.3"
	},
	"scripts": {
		"build:esbuild": "esbuild --packages=external --bundle --outfile=dist/cli.js --sourcemap --format=esm src/index.ts",
		"build": "yarn clean && yarn typecheck && yarn build:esbuild",
		"typecheck": "tsc",
		"clean": "rimraf dist",
		"start": "node dist/index",
		"fmt": "prettier --cache -w . && xo --fix"
	},
	"dependencies": {
		"@faker-js/faker": "^8.4.1",
		"camelcase": "^8.0.0",
		"commander": "^12.1.0"
	}
}
