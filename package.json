{
	"name": "faker-cli",
	"private": true,
	"type": "module",
	"packageManager": "yarn@3.3.1",
	"devDependencies": {
		"@lusc/tsconfig": "^2.0.0",
		"@types/node": "^18.11.18",
		"esbuild": "^0.17.2",
		"prettier": "^2.8.3",
		"rimraf": "^4.1.1",
		"typescript": "^4.9.4",
		"xo": "^0.53.1"
	},
	"scripts": {
		"build:esbuild": "esbuild --packages=external --outfile=dist/cli.js --format=esm src/index.ts",
		"build": "yarn clean && yarn typecheck && yarn build:esbuild",
		"typecheck": "tsc",
		"clean": "rimraf dist",
		"start": "node dist/index",
		"fmt": "prettier --cache -w . && xo --fix"
	},
	"dependencies": {
		"commander": "^10.0.0"
	}
}
