{
	"name": "faker-cli",
	"private": true,
	"type": "module",
	"packageManager": "yarn@3.6.0",
	"devDependencies": {
		"@lusc/tsconfig": "^2.3.0",
		"@types/node": "^20.2.5",
		"esbuild": "^0.18.4",
		"prettier": "^2.8.8",
		"rimraf": "^5.0.1",
		"type-fest": "^3.11.1",
		"typescript": "^5.1.3",
		"xo": "^0.54.2"
	},
	"scripts": {
		"build:esbuild": "esbuild --packages=external --bundle --outfile=dist/cli.js --sourcemap --format=esm src/index.ts",
		"build": "yarn clean && yarn typecheck && yarn build:esbuild",
		"typecheck": "tsc",
		"clean": "rimraf dist",
		"start": "node dist/index",
		"fmt": "prettier --cache -w . && xo --fix"
	},
	"dependencies": {
		"@faker-js/faker": "^8.0.2",
		"camelcase": "^7.0.1",
		"commander": "^11.0.0"
	}
}
