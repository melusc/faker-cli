{
	"name": "faker-cli",
	"private": true,
	"type": "module",
	"packageManager": "yarn@3.5.0",
	"devDependencies": {
		"@lusc/tsconfig": "^2.2.0",
		"@types/node": "^18.15.12",
		"esbuild": "^0.17.17",
		"prettier": "^2.8.7",
		"rimraf": "^5.0.0",
		"type-fest": "^3.8.0",
		"typescript": "^5.0.4",
		"xo": "^0.54.1"
	},
	"scripts": {
		"build:esbuild": "esbuild --packages=external --bundle --outfile=dist/cli.js --sourcemap --format=esm src/index.ts",
		"build": "yarn clean && yarn typecheck && yarn build:esbuild",
		"typecheck": "tsc",
		"clean": "rimraf dist",
		"start": "node dist/index",
		"fmt": "prettier --cache -w . && xo --fix"
	},
	"dependencies": {
		"@faker-js/faker": "^7.6.0",
		"camelcase": "^7.0.1",
		"commander": "^10.0.1"
	}
}
